{
    "type": "dark",
    "$schema": "vscode://schemas/color-theme",
    "name": "Safire Star",
    "semanticHighlighting": true,
    "colors": {
        "dropdown.listBackground": "#222222",
        "editor.background": "#282828",
        "editor.foreground": "#dddddd",

        "editor.selectionBackground": "#893619",
        "editor.inactiveSelectionBackground": "#613120", // Color of the selection in an inactive editor. The color must not be opaque so as not to hide underlying decorations.
        "editor.selectionHighlightBackground": "#9b3e1daa",
        "editor.findMatchBackground":"#893619",
        "editor.findMatchBorder":"#ef4713",
        "minimap.selectionHighlight": "#b94000",
        "editor.wordHighlightBackground": "#ff7700",
        "editor.wordHighlightStrongBackground": "#ff7700",
        "editor.lineHighlightBackground": "#343434",
        "editorLineNumber.activeForeground": "#cc96a4",
        "editorLineNumber.foreground": "#777777",
        "editorCursor.foreground": "#df4a16",
        "editorWhitespace.foreground": "#464741",
        "editorIndentGuide.background": "#464741",
        "editorIndentGuide.activeBackground": "#767771",
        "editorGroupHeader.tabsBackground": "#222222",
        "tab.inactiveBackground": "#222222",
        "tab.border": "#222222",
        "tab.inactiveForeground": "#aaaaaa", // n6eeds to be bright so it's readable when another editor group is focused
        "widget.shadow": "#00000098",
        "progressBar.background": "#75715E",
        "badge.foreground": "#f8f8f2",
        "panelTitle.activeForeground": "#f8f8f2",
        "panelTitle.activeBorder": "#75715E",
        "panelTitle.inactiveForeground": "#75715E",
        "titleBar.activeBackground": "#222222",
        "statusBar.debuggingBackground": "#75715E",
        "ports.iconRunningProcessForeground": "#ccccc7",
        "activityBar.background": "#282828",
        "activityBar.foreground": "#f8f8f2",
        "sideBar.background": "#222222",
        "sideBarSectionHeader.background": "#282828",
        "menu.background": "#222222",
        "menu.foreground": "#cccccc",
        "pickerGroup.foreground": "#75715E",
        "inputOption.activeBorder": "#75715E",
        "editorWidget.background": "#222222",
        "debugToolBar.background": "#222222",
        "diffEditor.insertedTextBackground": "#4b661680", // middle of #282828 and #a6e22e
        "diffEditor.removedTextBackground": "#90274A70", // middle of #282828 and #f92672
        "inputValidation.errorBackground": "#90274A", // middle of #282828 and #f92672
        "inputValidation.errorBorder": "#f92672",
        "inputValidation.warningBackground": "#848528", // middle of #282828 and #e2e22e
        "inputValidation.warningBorder": "#e2e22e",
        "inputValidation.infoBorder": "#819aff",
        "editorHoverWidget.border": "#75715E",
        "editorSuggestWidget.background": "#282828",
        "editorSuggestWidget.border": "#75715E",
        "editorGroup.border": "#34352f",
        "peekView.border": "#75715E",
        "peekViewEditor.background": "#282828",
        "peekViewResult.background": "#222222",
        "peekViewTitle.background": "#222222",
        "peekViewResult.matchHighlightBackground": "#75715E",
        "peekViewEditor.matchHighlightBackground": "#75715E",
        "terminal.ansiBlack": "#333333",
        "terminal.ansiRed": "#C4265E", // the bright color with ~75% transparent on the background
        "terminal.ansiGreen": "#86B42B",
        "terminal.ansiYellow": "#B3B42B",
        "terminal.ansiBlue": "#6A7EC8",
        "terminal.ansiMagenta": "#8C6BC8",
        "terminal.ansiCyan": "#56ADBC",
        "terminal.ansiWhite": "#e3e3dd",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiBrightRed": "#f92672",
        "terminal.ansiBrightGreen": "#b1e942",
        "terminal.ansiBrightYellow": "#e2e22e", // hue shifted #b1e942
        "terminal.ansiBrightBlue": "#819aff", // hue shifted #AE81FF
        "terminal.ansiBrightMagenta": "#AE81FF",
        "terminal.ansiBrightCyan": "#7ee1f5",
        "terminal.ansiBrightWhite": "#f8f8f2",

        "descriptionForeground":"#cf274b",
        "editorHint.foreground":"#cf274b",
        "statusBarItem.remoteBackground": "#cf274b",
        "activityBarBadge.background": "#cf274b",
        "editorOverviewRuler.infoForeground": "#cf274b",
        "focusBorder": "#cf274b",
        "badge.background":"#cf274b",
        "selection.background": "#af233a88",

        // this is the colors for the command pallete and sidebar selected item
        "list.hoverBackground": "#484848",
        "list.focusBackground": "#361d22",
        "list.activeSelectionBackground":"#5e1a23",
        "list.inactiveSelectionBackground": "#421d22",
        "button.background":"#cf274b",
        // this is the text that shows up in "Quick fix" links
        "textLink.activeForeground":"#ca3958",
        "textLink.foreground":"#ca3958",
        "list.highlightForeground":"#ca3958", // the text color of hints in command palette
        "editorSuggestWidget.selectedBackground": "#484848", // the color of the active code completion entry
        "editorSuggestWidget.highlightForeground": "#e0506f", // the color of code completion popup text hints. Make theis match list.highlightForeground" but maybe a little brighter and contrasting since the text is small

        "dropdown.background": "#47242a",
        "quickInputList.focusBackground": "#484848", // the background of command pallete selection
        // "list.dropBackground": "#cf274b",
        // "editorGroup.dropBackground": "#41433980",co
        "tab.lastPinnedBorder": "#cf274b",
        "panel.border": "#555555",
        "settings.focusedRowBackground": "#4143395A",
        "statusBar.background": "#cf274b",
        "statusBar.noFolderBackground": "#cf274b",
        "input.background": "#181818",
        "editorHoverWidget.background": "#222222",
        "peekViewResult.selectionBackground": "#cf274b",
    },
    "tokenColors": [
        {
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "name": "Template Definition",
            "scope": [
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#ee4c61"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#ee4c61"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#ee4c61"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#b1e942"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#ee4c61"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#b1e942"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#7ee1f5"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#7ee1f5"
            }
        },
        {
            "name": "Library class/type",
            "scope": "support.type, support.class",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7ee1f5"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#F44747"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#F44747"
            }
        },
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#CFCFC2"
            }
        },
        {
            "name": "diff.header",
            "scope": "meta.diff, meta.diff.header",
            "settings": {
                "foreground": "#75715E"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ee4c61"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#b1e942"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#AE81FFA0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#ee4c61"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "name": "Markup Styling",
            "scope": "markup.bold, markup.italic",
            "settings": {
                "foreground": "#7ee1f5"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#FD971F"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#b1e942"
            }
        },
        {
            "name": "Markup Setext Header",
            "scope": "markup.heading.setext",
            "settings": {
                "foreground": "#b1e942",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Quote",
            "scope": "markup.quote.markdown",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#75715E"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Link Title/Description",
            "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
            "settings": {
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "Markdown Underline Link/Image",
            "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "name": "Markdown Emphasis",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Markdown Punctuation Definition Link",
            "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
            "settings": {
                "foreground": "#f8f8f2"
            }
        },
        {
            "name": "Markdown List Punctuation",
            "scope": [
                "punctuation.definition.list.begin.markdown"
            ],
            "settings": {
                "foreground": "#b1e942"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#b267e6"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#FD971F"
            }
        },
        { 
            "name": "Storage type",
            "scope": "storage.type", // this is what decides annotation color like @override (at least in dart)
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FD971F"
            }
        },
    ],
}